{
  "name": "compreaqui-ecommerce",
  "version": "1.0.0",
  "description": "E-commerce moderno CompreAqui com backend Node.js e SQLite",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "start:production": "NODE_ENV=production node start-production.js",
    "dev": "nodemon server.js",
    "build": "webpack --mode=production",
    "build:dev": "webpack --mode=development",
    "build:analyze": "webpack --mode=production --env analyze",
    "serve": "webpack serve --mode=development",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:e2e": "cypress run",
    "test:e2e:open": "cypress open",
    "test:payment": "node tests/test-runner.js",
    "test:payment:unit": "mocha tests/payment-tests.js --timeout 10000",
    "test:payment:integration": "mocha tests/integration-tests.js --timeout 15000",
    "test:payment:watch": "nodemon --exec \"npm run test:payment\"",
    "test:payment:coverage": "nyc npm run test:payment",
    "lint": "eslint js/**/*.js",
    "lint:fix": "eslint js/**/*.js --fix",
    "format": "prettier --write .",
    "clean": "rimraf dist",
    "prestart": "npm run build",
    "postinstall": "npm run build",
    "prepare": "husky install",
    "setup:production": "node scripts/setup-production.js",
    "setup:payment": "node scripts/setup-payment-system.js",
    "health:check": "node scripts/health-check.js",
    "health:monitor": "node scripts/health-check.js --monitor",
    "ssl:generate": "node -e \"const SecuritySystem = require('./security'); const s = new SecuritySystem(); s.initialize().then(() => s.sslManager.generateSelfSignedCertificate())\"",
    "ssl:renew": "node -e \"const SecuritySystem = require('./security'); const s = new SecuritySystem(); s.initialize().then(() => s.renewCertificates())\"",
    "security:report": "node -e \"const SecuritySystem = require('./security'); const s = new SecuritySystem(); s.initialize().then(() => console.log(JSON.stringify(s.generateSecurityReport(), null, 2)))\""
  },
  "keywords": [
    "ecommerce",
    "nodejs",
    "sqlite",
    "express",
    "javascript",
    "compreaqui"
  ],
  "author": "CompreAqui Team",
  "license": "MIT",
  "dependencies": {
    "express": "^4.18.2",
    "cors": "^2.8.5",
    "sqlite3": "^5.1.6",
    "bcrypt": "^5.1.0",
    "jsonwebtoken": "^9.0.0",
    "dotenv": "^16.0.3",
    "express-rate-limit": "^6.7.0",
    "helmet": "^6.1.5",
    "winston": "^3.8.2",
    "express-winston": "^4.2.0",
    "compression": "^1.7.4",
    "express-validator": "^6.15.0",
    "bcryptjs": "^2.4.3",
    "qrcode": "^1.5.3",
    "moment": "^2.29.4",
    "validator": "^13.11.0",
    "mysql2": "^3.6.0",
    "sequelize": "^6.32.1",
    "nodemailer": "^6.9.4",
    "axios": "^1.5.0",
    "lodash": "^4.17.21",
    "uuid": "^9.0.0",
    "multer": "^1.4.5-lts.1",
    "sharp": "^0.32.5",
    "pdf-lib": "^1.17.1",
    "node-cron": "^3.0.2",
    "express-slow-down": "^1.6.0",
    "express-brute": "^1.0.1",
    "xss": "^1.0.14",
    "dompurify": "^3.0.5",
    "jsdom": "^22.1.0"
  },
  "devDependencies": {
    "nodemon": "^2.0.22",
    "jest": "^29.5.0",
    "supertest": "^6.3.3",
    "cypress": "^12.10.0",
    "mocha": "^10.2.0",
    "chai": "^4.3.8",
    "nyc": "^15.1.0",
    "sinon": "^15.2.0",
    "proxyquire": "^2.1.3",
    "cross-env": "^7.0.3",
    "concurrently": "^8.2.1",
    "webpack": "^5.82.1",
    "webpack-cli": "^5.1.1",
    "webpack-dev-server": "^4.15.0",
    "html-webpack-plugin": "^5.5.1",
    "mini-css-extract-plugin": "^2.7.5",
    "css-minimizer-webpack-plugin": "^5.0.0",
    "terser-webpack-plugin": "^5.3.7",
    "clean-webpack-plugin": "^4.0.0",
    "compression-webpack-plugin": "^10.0.0",
    "webpack-bundle-analyzer": "^4.8.0",
    "@babel/core": "^7.21.8",
    "@babel/preset-env": "^7.21.5",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-proposal-class-properties": "^7.18.6",
    "@babel/plugin-proposal-optional-chaining": "^7.21.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.18.6",
    "babel-loader": "^9.1.2",
    "babel-plugin-transform-remove-console": "^6.9.4",
    "css-loader": "^6.7.4",
    "style-loader": "^3.3.2",
    "postcss": "^8.4.23",
    "postcss-loader": "^7.3.0",
    "autoprefixer": "^10.4.14",
    "cssnano": "^6.0.1",
    "@fullhuman/postcss-purgecss": "^5.0.0",
    "core-js": "^3.30.2"
  },
  "engines": {
    "node": ">=16.0.0",
    "npm": ">=8.0.0"
  }
}