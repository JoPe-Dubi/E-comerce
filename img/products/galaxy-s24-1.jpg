<svg width="300" height="300" viewBox="0 0 300 300" fill="none" xmlns="http://www.w3.org/2000/svg">
  <!-- Background -->
  <rect width="300" height="300" fill="#F8F9FA"/>
  
  <!-- Phone Body -->
  <rect x="80" y="35" width="140" height="230" rx="22" fill="url(#galaxyGradient)" stroke="#2C2C2E" stroke-width="1"/>
  
  <!-- Screen -->
  <rect x="85" y="45" width="130" height="210" rx="18" fill="#000000"/>
  
  <!-- Screen Content -->
  <rect x="90" y="55" width="120" height="190" rx="15" fill="url(#galaxyScreen)"/>
  
  <!-- Samsung UI Elements -->
  <g transform="translate(95, 65)">
    <!-- Status Bar -->
    <rect x="0" y="0" width="110" height="2" rx="1" fill="#FFFFFF" opacity="0.8"/>
    
    <!-- Time -->
    <rect x="5" y="5" width="20" height="3" rx="1.5" fill="#FFFFFF" opacity="0.9"/>
    
    <!-- Battery and Signal -->
    <rect x="85" y="5" width="8" height="3" rx="1" fill="#FFFFFF" opacity="0.9"/>
    <rect x="95" y="5" width="10" height="3" rx="1.5" fill="#34C759"/>
    
    <!-- App Icons -->
    <!-- Row 1 -->
    <circle cx="15" cy="25" r="8" fill="#1976D2"/>
    <circle cx="35" cy="25" r="8" fill="#E63946"/>
    <circle cx="55" cy="25" r="8" fill="#4CAF50"/>
    <circle cx="75" cy="25" r="8" fill="#FF9800"/>
    <circle cx="95" cy="25" r="8" fill="#9C27B0"/>
    
    <!-- Row 2 -->
    <circle cx="15" cy="45" r="8" fill="#F44336"/>
    <circle cx="35" cy="45" r="8" fill="#2196F3"/>
    <circle cx="55" cy="45" r="8" fill="#FF5722"/>
    <circle cx="75" cy="45" r="8" fill="#795548"/>
    <circle cx="95" cy="45" r="8" fill="#607D8B"/>
    
    <!-- Samsung One UI Widget -->
    <rect x="10" y="65" width="90" height="40" rx="8" fill="rgba(255,255,255,0.1)" stroke="rgba(255,255,255,0.2)" stroke-width="0.5"/>
    <rect x="15" y="70" width="80" height="4" rx="2" fill="#FFFFFF" opacity="0.7"/>
    <rect x="15" y="78" width="60" height="3" rx="1.5" fill="#FFFFFF" opacity="0.5"/>
    <rect x="15" y="85" width="70" height="3" rx="1.5" fill="#FFFFFF" opacity="0.5"/>
    <rect x="15" y="92" width="50" height="3" rx="1.5" fill="#FFFFFF" opacity="0.5"/>
    
    <!-- Navigation Bar -->
    <rect x="20" y="165" width="70" height="15" rx="7" fill="rgba(255,255,255,0.1)"/>
    <circle cx="30" cy="172" r="3" fill="#FFFFFF" opacity="0.6"/>
    <rect x="47" y="169" width="6" height="6" rx="1" fill="#FFFFFF" opacity="0.6"/>
    <circle cx="70" cy="172" r="3" fill="#FFFFFF" opacity="0.6"/>
  </g>
  
  <!-- Camera Module -->
  <g transform="translate(90, 50)">
    <rect x="0" y="0" width="30" height="30" rx="6" fill="#1C1C1E"/>
    
    <!-- Main Camera -->
    <circle cx="8" cy="8" r="5" fill="#2C2C2E" stroke="#48484A" stroke-width="0.5"/>
    <circle cx="8" cy="8" r="3" fill="#000000"/>
    <circle cx="9" cy="7" r="0.8" fill="#FFFFFF" opacity="0.4"/>
    
    <!-- Ultra-wide Camera -->
    <circle cx="22" cy="8" r="4" fill="#2C2C2E" stroke="#48484A" stroke-width="0.5"/>
    <circle cx="22" cy="8" r="2.5" fill="#000000"/>
    <circle cx="22.5" cy="7.5" r="0.6" fill="#FFFFFF" opacity="0.4"/>
    
    <!-- Telephoto Camera -->
    <circle cx="8" cy="22" r="4" fill="#2C2C2E" stroke="#48484A" stroke-width="0.5"/>
    <circle cx="8" cy="22" r="2.5" fill="#000000"/>
    <circle cx="8.5" cy="21.5" r="0.6" fill="#FFFFFF" opacity="0.4"/>
    
    <!-- Flash -->
    <circle cx="22" cy="22" r="2" fill="#FFE082"/>
    <circle cx="22" cy="22" r="1" fill="#FFF59D"/>
  </g>
  
  <!-- Side Buttons -->
  <rect x="75" y="90" width="3" height="12" rx="1.5" fill="#2C2C2E"/>
  <rect x="75" y="110" width="3" height="20" rx="1.5" fill="#2C2C2E"/>
  <rect x="75" y="135" width="3" height="20" rx="1.5" fill="#2C2C2E"/>
  
  <!-- USB-C Port -->
  <rect x="142" y="260" width="16" height="4" rx="2" fill="#2C2C2E"/>
  
  <!-- Speaker Grilles -->
  <g transform="translate(90, 45)">
    <circle cx="5" cy="0" r="0.8" fill="#2C2C2E"/>
    <circle cx="10" cy="0" r="0.8" fill="#2C2C2E"/>
    <circle cx="15" cy="0" r="0.8" fill="#2C2C2E"/>
    <circle cx="20" cy="0" r="0.8" fill="#2C2C2E"/>
    <circle cx="25" cy="0" r="0.8" fill="#2C2C2E"/>
  </g>
  
  <!-- Gradients -->
  <defs>
    <linearGradient id="galaxyGradient" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1A1A1A;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#2C2C2E;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#000000;stop-opacity:1" />
    </linearGradient>
    
    <linearGradient id="galaxyScreen" x1="0%" y1="0%" x2="100%" y2="100%">
      <stop offset="0%" style="stop-color:#1E3A8A;stop-opacity:1" />
      <stop offset="50%" style="stop-color:#1E40AF;stop-opacity:1" />
      <stop offset="100%" style="stop-color:#1E1B4B;stop-opacity:1" />
    </linearGradient>
  </defs>
  
  <!-- Screen Reflection -->
  <rect x="85" y="45" width="65" height="210" rx="18" fill="url(#screenReflection)" opacity="0.1"/>
  
  <defs>
    <linearGradient id="screenReflection" x1="0%" y1="0%" x2="100%" y2="0%">
      <stop offset="0%" style="stop-color:#FFFFFF;stop-opacity:0.4" />
      <stop offset="100%" style="stop-color:#FFFFFF;stop-opacity:0" />
    </linearGradient>
  </defs>
</svg>